+__s32 _wait_cmdfifo_free(void)
+{
+	__s32 timeout = 0xffff;
+
+	while ( (timeout--) && (NFC_READ_REG(NFC_REG_ST) & NFC_CMD_FIFO_STATUS) );
+	if (timeout <= 0)
+		return -ERR_TIMEOUT;
+	return 0;
+}
