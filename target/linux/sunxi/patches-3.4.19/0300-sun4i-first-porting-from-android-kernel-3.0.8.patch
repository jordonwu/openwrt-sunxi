+void _set_addr(__u8 *addr, __u8 cnt)
+{
+	__u32 i;
+	__u32 addr_low = 0;
+	__u32 addr_high = 0;
+
+	for (i = 0; i < cnt; i++){
+		if (i < 4)
+			addr_low |= (addr[i] << (i*8) );
+		else
+			addr_high |= (addr[i] << ((i - 4)*8));
