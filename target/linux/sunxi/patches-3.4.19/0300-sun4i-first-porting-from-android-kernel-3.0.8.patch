+static int nand_flush(struct nand_blk_dev *dev)
+{
+	if (0 == down_trylock(&mytr.nand_ops_mutex))
+	{
+		IS_IDLE = 0;
+	#ifdef NAND_CACHE_RW
+		NAND_CacheFlush();
+	#else
+		LML_FlushPageCache();
+	#endif
+		up(&mytr.nand_ops_mutex);
+		IS_IDLE = 1;
+
+		dbg_inf("nand_flush \n");
+	}
+	return 0;
+}
