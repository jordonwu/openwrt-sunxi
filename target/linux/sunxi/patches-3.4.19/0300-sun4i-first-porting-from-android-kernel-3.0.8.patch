+        TMR_REG_TMR1_CTL |= (1<<2);
+    #else
+        TMR_REG_TMR1_CTL |= (0<<2);
+    #endif
+    /* reload inter value */
+    TMR_REG_TMR1_CTL |= (1<<1);
+    /* install timer irq */
+    setup_irq(SW_INT_IRQNO_TIMER1, &aw_clkevt_irqact);
+    /* enable timer1 irq            */
