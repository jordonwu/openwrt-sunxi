+/********************************************************************
+***************************wait rb ready*******************************
+*********************************************************************/
+__s32 _wait_rb_ready(__u32 chip)
+{
+	__s32 timeout = 0xffff;
+	__u32 rb;
+
+
+      rb = _cal_real_rb(chip);
+
+
+	/*wait rb ready*/
+	while((timeout--) && (NFC_CheckRbReady(rb)));
+	if (timeout < 0)
+		return -ERR_TIMEOUT;
+
+	return 0;
+}
