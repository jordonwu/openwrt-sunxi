+	_add_cmd_list(cmd_list + 3,0x30,NFC_IGNORE,NFC_IGNORE,NFC_IGNORE,NFC_IGNORE,NFC_IGNORE,NFC_IGNORE);
+	list_len = 4;
+	for(i = 0; i < list_len - 1; i++){
+		cmd_list[i].next = &(cmd_list[i+1]);
