+            /* wait hardware synchronization, 2 cycles of the hardware work clock at least  */
+            __delay(50);
+            TMR_REG_TMR1_CTL &= ~(1<<7);
+            TMR_REG_TMR1_CTL |= (1<<0);
+            break;
+        }
+
+        case CLOCK_EVT_MODE_ONESHOT:
+        {
+            /* set timer work with onshot mode */
+            TMR_REG_TMR1_CTL &= ~(1<<0);
+            /* wait hardware synchronization, 2 cycles of the hardware work clock at least  */
+            __delay(50);
+            TMR_REG_TMR1_CTL |= (1<<7);
+            TMR_REG_TMR1_CTL |= (1<<0);
+            break;
+        }
+
+        default:
+        {
+            /* disable clock event device */
+            TMR_REG_TMR1_CTL &= ~(1<<0);
+            /* wait hardware synchronization, 2 cycles of the hardware work clock at least  */
+            __delay(50);
+            break;
+        }
+    }
+}
+#endif
