+		ret = NFC_Read_1K(cmd_list, readop->mainbuf, sparebuf, dma_wait_mode , NFC_PAGE_MODE);
+	}
+
+
+
+	if (dma_wait_mode)
+		_pending_dma_irq_sem();
+
+	if (readop->oobbuf){
+		MEMCPY(readop->oobbuf,sparebuf, 2 * 4);
+	}
+
+	NFC_DeSelectChip(readop->chip);
+	NFC_DeSelectRb(rb);
+
+	//FREE(sparebuf);
