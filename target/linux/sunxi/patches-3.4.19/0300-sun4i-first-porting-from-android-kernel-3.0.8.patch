+static void nand_flush_all(void)
+{
+    int     timeout = 0;
+
+    /* wait write finish */
+    for(timeout=0; timeout<10; timeout++) {
+        if(after_write) {
+            msleep(500);
+        }
+        else {
+            break;
+        }
+    }
+    printk("nand try to shutdown %d time\n", timeout);
+
+    /* get nand ops mutex */
+    down(&mytr.nand_ops_mutex);
+
+    #ifdef NAND_CACHE_RW
+    NAND_CacheFlush();
+    #else
+    LML_FlushPageCache();
+    #endif
+    BMM_WriteBackAllMapTbl();
+    printk("Nand flash shutdown ok!\n");
+}
