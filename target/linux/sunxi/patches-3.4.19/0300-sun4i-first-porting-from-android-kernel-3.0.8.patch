+static __aw_ccu_sys_clk_e sys_clk_get_parent(__aw_ccu_sys_clk_e id)
+{
+    switch(id)
+    {
+        case AW_SYS_CLK_PLL2X8:
+        {
+            return AW_SYS_CLK_PLL2;
+        }
+        case AW_SYS_CLK_PLL3X2:
+        {
+            return AW_SYS_CLK_PLL3;
+        }
+        case AW_SYS_CLK_PLL7X2:
+        {
+            return AW_SYS_CLK_PLL7;
+        }
