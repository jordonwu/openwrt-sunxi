+    char               *tmp_buf;                                        //转换成char类型
+    __u32               group_count_max;                                //最大GPIO个数
+    system_gpio_set_t  *user_gpio_set = NULL, *tmp_sys_gpio_data;
+    volatile __u32      *tmp_group_pull_addr = NULL;
+    __u32               port, port_num, port_num_pull;
