+        case AW_SYS_CLK_PLL5P:
+        {
+            return AW_SYS_CLK_PLL5;
+        }
+        case AW_SYS_CLK_200M:
+        {
+            return AW_SYS_CLK_PLL62;
+        }
+        case AW_SYS_CLK_CPU:
+        {
+            switch(aw_ccu_reg->SysClkDiv.AC328ClkSrc)
+            {
+                case 0:
