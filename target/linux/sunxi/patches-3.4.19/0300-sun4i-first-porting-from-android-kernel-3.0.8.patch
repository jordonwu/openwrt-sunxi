+__s32 _wait_cmd_finish(void)
+{
+	__s32 timeout = 0xffff;
+	while( (timeout--) && !(NFC_READ_REG(NFC_REG_ST) & NFC_CMD_INT_FLAG) );
+	if (timeout <= 0)
+		return -ERR_TIMEOUT;
+
+	NFC_WRITE_REG(NFC_REG_ST, NFC_READ_REG(NFC_REG_ST) & NFC_CMD_INT_FLAG);
+	return 0;
+}
