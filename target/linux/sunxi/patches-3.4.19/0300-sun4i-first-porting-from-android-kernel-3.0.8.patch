+static void aw_twi_release_gpio(struct sun4i_i2c *i2c)
+{
+	if(i2c->bus_num == 0) {
+		/* pb0-pb1 TWI0 SDA,SCK */
+		gpio_release(i2c->gpio_hdle, 0);
+	}
+	else if(i2c->bus_num == 1) {
+		/* pb18-pb19 TWI1 scl,sda */
+		gpio_release(i2c->gpio_hdle, 0);
+	}
+	else if(i2c->bus_num == 2) {
+		/* pb20-pb21 TWI2 scl,sda */
+		gpio_release(i2c->gpio_hdle, 0);
+	}
+}
