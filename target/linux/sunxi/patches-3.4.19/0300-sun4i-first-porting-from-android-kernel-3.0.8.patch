+	start = blk_rq_pos(req);
+	nsector = blk_rq_cur_bytes(req)>>9;
+
+	if ( (start + nsector) > get_capacity(req->rq_disk))
+	{
+		dbg_err("over the limit of disk\n");
+
+		return -EIO;
+	}
+	start += dev->off_size;
+
+	switch(cmd) {
+
