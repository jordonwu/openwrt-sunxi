+	if(dev->disable_access || ( (cmd == WRITE) && (dev->readonly) ) \
+		|| ((cmd == READ) && (dev->writeonly))){
+		dbg_err("can not access this part\n");
+
+		return -EIO;
+	}
+
+
+	//for2.6.36
