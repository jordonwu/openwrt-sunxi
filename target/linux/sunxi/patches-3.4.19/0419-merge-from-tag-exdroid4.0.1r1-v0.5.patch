+	buf[PCF8563_REG_AHR] = (0<<7);
+	buf[PCF8563_REG_ADM] = (0<<7);
+	buf[PCF8563_REG_ADW] = (1<<7);
+		/* write register's data */
+	for (i = 0; i < 4; i++) {
