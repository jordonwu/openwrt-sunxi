-#define TRX_OFFSETS_JUMPTO_IDX	1	/* RAM address for jumpto after download */
-#define TRX_OFFSETS_NVM_LEN_IDX	2	/* Length of appended NVRAM data */
-
-#define TRX_OFFSETS_DLBASE_IDX  0       /* RAM start address for download */
-
-#endif  /* _USB_RDL_H */
