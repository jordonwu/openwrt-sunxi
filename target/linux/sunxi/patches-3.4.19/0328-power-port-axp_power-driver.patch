+		.name = "axp19-regulator",
+		.id = AXP19_ID_BUCK3,
+		.platform_data = &axp_regl_init_data[vcc_buck3],
+	}, {
+		.name = "axp19-regulator",
+		.id = AXP19_ID_LDOIO0,
+		.platform_data = &axp_regl_init_data[vcc_ldoio0],
