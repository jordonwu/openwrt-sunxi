+
+static inline int axp20_vdc_to_mV(uint16_t reg)
+{
+  return ((int)(((reg >> 8) << 4 ) | (reg & 0x000F))) * 1700 / 1000;
+}
+
+
+static inline int axp20_ibat_to_mA(uint16_t reg)
+{
+    return ((int)(((reg >> 8) << 5 ) | (reg & 0x001F))) * 500 / 1000;
