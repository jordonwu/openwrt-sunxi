+      val &= ~(3 << 5);
+      val |= 1 << 6;
+      }
+    else
+      val |= 3 << 5;
+
+		if(charger->chgcur == 0)
+			charger->chgen = 0;
+
+    if(charger->chgcur< 300000)
+      charger->chgcur = 300000;
+    else if(charger->chgcur > 1800000)
+     charger->chgcur = 1800000;
+
+    val |= (charger->chgcur - 200001) / 100000 ;
+    if(charger ->chgend == 10){
+      val &= ~(1 << 4);
+    }
+    else {
+      val |= 1 << 4;
+    }
+    val &= 0x7F;
