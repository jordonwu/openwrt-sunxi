+int ADC_Freq_Get(struct axp_charger *charger)
+{
+	uint8_t  temp;
+	int  rValue = 25;
+
+	axp_read(charger->master, AXP20_ADC_CONTROL3,&temp);
+	temp &= 0xc0;
+	switch(temp >> 6)
+	{
+		case 0:
+			rValue = 25;
+			break;
+		case 1:
+			rValue = 50;
+			break;
+		case 2:
+			rValue = 100;
+			break;
+		case 3:
+			rValue = 200;
+			break;
+		default:
+			break;
+	}
+	return rValue;
+}
