+    tmp = ((((charger->chgpretime - 40) / 10) << 6)  \
+      | ((charger->chgcsttime - 360) / 120));
+	axp_write(charger->master, AXP20_CHARGE_CONTROL1,val);
+	axp_update(charger->master, AXP20_CHARGE_CONTROL2,tmp,0xC2);
