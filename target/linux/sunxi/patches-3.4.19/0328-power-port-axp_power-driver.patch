+  struct axp_charger *charger = dev_get_drvdata(dev);
+  int var;
+  uint8_t tmp, val;
+  var = simple_strtoul(buf, NULL, 10);
+  switch(var){
+    case 4100000:tmp = 0;break;
+    case 4150000:tmp = 1;break;
+    case 4200000:tmp = 2;break;
