+static int axp_battery_event(struct notifier_block *nb, unsigned long event,
+				void *data)
+{
+	struct axp_charger *charger =
+		container_of(nb, struct axp_charger, nb);
+
+	axp_charger_update_state(charger);
+
+	switch (event) {
