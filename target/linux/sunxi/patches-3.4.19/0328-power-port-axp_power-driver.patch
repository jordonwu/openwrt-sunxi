+{
+	struct axp_charger *charger;
+
+	charger = container_of(work, struct axp_charger, work.work);
+
+	axp_charger_update_state(charger);
+	axp_charger_update(charger);
+
+
+	schedule_delayed_work(&charger->work, charger->interval);
+}
