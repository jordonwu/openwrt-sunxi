+int axp_charger_create_attrs(struct power_supply *psy)
+{
+	int j,ret;
+	for (j = 0; j < ARRAY_SIZE(axp_charger_attrs); j++) {
+		ret = device_create_file(psy->dev,
+			    &axp_charger_attrs[j]);
+		if (ret)
+			goto sysfs_failed;
+	}
+    goto succeed;
+
+sysfs_failed:
+	while (j--)
+		device_remove_file(psy->dev,
+			   &axp_charger_attrs[j]);
+succeed:
+	return ret;
