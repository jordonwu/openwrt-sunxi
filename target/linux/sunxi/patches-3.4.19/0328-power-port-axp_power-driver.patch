+static int axp_bat_vol(bool Flag,int Bat_Vol,int Bat_Cur,uint16_t Rdc)
+{
+    if(Flag)
+    {
+        return Bat_Vol- (Bat_Cur*(int)Rdc/1000);
+    }
+    else
+    {
+        return Bat_Vol+ (Bat_Cur*(int)Rdc/1000);
+    }
+}
