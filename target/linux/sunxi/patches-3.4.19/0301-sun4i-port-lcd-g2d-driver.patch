+    LCD_GPIO_set_attr(sel, 0, 1);
+    LCD_GPIO_write(sel, 0, 1);
+
+    //request SDAB gpio, and output high as default
+    LCD_GPIO_request(sel, 1);
+    LCD_GPIO_set_attr(sel, 1, 1);
+    LCD_GPIO_write(sel, 1, 1);
