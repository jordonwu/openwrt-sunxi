+
+int OSAL_sw_get_ic_ver(void)
+{
+    enum sw_ic_ver ic_ver;
+    int ret = 0xA;
+
+    ic_ver = sw_get_ic_ver();
+    if(ic_ver == MAGIC_VER_A)
+    {
+        ret = 0xA;
+    }
+    else if(ic_ver == MAGIC_VER_B)
+    {
+        ret = 0xB;
+    }
+    if(ic_ver == MAGIC_VER_C)
+    {
+        ret = 0xC;
+    }
+
+    return ret;
+}
