+    if(gpanel_info[sel].tcon_index == 0)
+    {
+        TCON0_cfg(sel,(__panel_para_t*)&gpanel_info[sel]);
+    }
+    else
+    {
+        TCON1_cfg_ex(sel,(__panel_para_t*)&gpanel_info[sel]);
