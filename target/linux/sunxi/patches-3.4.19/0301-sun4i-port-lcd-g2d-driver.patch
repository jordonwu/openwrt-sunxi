+	TVE_dac_int_disable(sel,1);
+	TVE_dac_int_disable(sel,2);
+	TVE_dac_int_disable(sel,3);
+	TVE_dac_autocheck_enable(sel,0);
+	TVE_dac_autocheck_enable(sel,1);
+	TVE_dac_autocheck_enable(sel,2);
+	TVE_dac_autocheck_enable(sel,3);
+	TVE_csc_init(sel,0);
+
+	if(sel == 0)
+	{
+	    TVE_dac_sel(0, 0, 0);
+	    TVE_dac_sel(0, 1, 1);
+	    TVE_dac_sel(0, 2, 2);
