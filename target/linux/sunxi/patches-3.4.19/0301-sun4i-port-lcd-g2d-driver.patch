+    BSP_disp_set_yuv_output(sel, FALSE);
+    DE_BE_set_display_size(sel, gpanel_info[sel].lcd_x, gpanel_info[sel].lcd_y);
+    DE_BE_Output_Select(sel, sel);
+
+    open_flow[sel].func_num = 0;
