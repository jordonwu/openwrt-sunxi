+static void i2cBStop(void)
+{
+   IIC_SDAB_OUTPUT_SETUP();
+   IIC_SDAB_LOW();
+   LCD_delay_us(5) ;
+   IIC_SCLB_HIGH();
+   LCD_delay_us(5) ;
+   IIC_SDAB_HIGH();
+   LCD_delay_us(5) ;
