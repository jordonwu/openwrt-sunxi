+static __bool i2cBTransmit(__u8 value)
+{
+	register __u8 i ;
+	IIC_SDAB_OUTPUT_SETUP();
+	LCD_delay_us(5) ;
+	for ( i=0 ; i<8 ; i++ )
+	{
+		if((value&0x80)==0x80)
+		{
+			IIC_SDAB_HIGH();
+			//printk("//////// DATA-1  //////\n");
+		}
+		else
+		{
+			IIC_SDAB_LOW();
+			//printk("//////// DATA-0  //////\n");
+		}
+		value = value << 1 ;
+		LCD_delay_us(10) ;
+		i2cB_clock() ;
+
+	}
+	return(!i2cB_ack());
+}
+
+static __bool i2cBTransmitSubAddr(__u8 value)
+{
+	register __u8 i ;
+
+	for ( i=0 ; i<8 ; i++ )
+	{
+		if((value&0x80)==0x80)
+		{
+			IIC_SDAB_HIGH();
+		}
+		else
+		{
+			IIC_SDAB_LOW();
+		}
+		value = value << 1 ;
+		LCD_delay_us(10) ;
+		i2cB_clock() ;
+	}
+	return(!i2cB_ack());
+}
