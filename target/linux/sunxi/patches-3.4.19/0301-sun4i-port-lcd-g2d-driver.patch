+static void rect_multi(__s32 *dest, __s32 *src1, __s32 *src2)
+{
+	__u32 x,y,z;
+	__s64 val_int64;
+
+	for(x=0;x<4;x++)
+		for(y=0;y<4;y++)
+		{
+			val_int64 = 0;
+			for(z=0;z<4;z++)
+				val_int64 += (__s64)src1[x*4+z]*src2[z*4+y];
+			val_int64 = (val_int64+512)>>10;
+			dest[x*4+y] = val_int64;
+		}
+}
