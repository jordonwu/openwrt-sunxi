+	/* for interleaved yuv422 */
+    G2D_SEQ_VYUY   = 0x1,				/* pixel 016 */
+    G2D_SEQ_YVYU   = 0x2,				/* pixel 116 */
+
+	/* for uv_combined yuv420 */
+    G2D_SEQ_VUVU   = 0x3,
