+// parameters       :
+//                 sel <scaler select>
+//                 out<0:be0; 1:be1; 2:me; 3:writeback>
+// return            : success
+//***********************************************************************************
+__s32 DE_SCAL_Output_Select(__u8 sel, __u8 out)
+{
