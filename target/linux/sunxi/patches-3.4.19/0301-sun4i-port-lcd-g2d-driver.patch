+static __bool i2cB_clock( void )
+{
+	__bool sample = 0;
+
+	IIC_SCLB_HIGH();
+	LCD_delay_us(10) ;
+	IIC_SCLB_LOW();
+	LCD_delay_us(10) ;
+	return ( sample ) ;
+}
+
+static __bool i2cB_ack(void)
+{
+	IIC_SCLB_HIGH();
+	IIC_SDAB_INPUT_SETUP();
+	LCD_delay_us(5);
+	LCD_delay_us(5);
+	if(CHECK_SDAB_HIGH())
+	{
+		LCD_delay_us(5) ;
+		IIC_SDAB_OUTPUT_SETUP();
+		LCD_delay_us(5) ;
+		IIC_SCLB_LOW();
+		LCD_delay_us(5) ;
+		IIC_SDAB_HIGH();
+		LCD_delay_us(5) ;
+		return(1);
+	}
+	else
+	{
+		LCD_delay_us(5) ;
+		IIC_SDAB_OUTPUT_SETUP();
+		LCD_delay_us(5) ;
+		IIC_SCLB_LOW();
+		LCD_delay_us(5) ;
+		IIC_SDAB_HIGH();
+		LCD_delay_us(5) ;
+		return(0);
+	}
