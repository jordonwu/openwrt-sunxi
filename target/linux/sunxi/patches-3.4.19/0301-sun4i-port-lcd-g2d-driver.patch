+static void i2cBStartA( void )
+{
+	IIC_SCLB_HIGH();
+	IIC_SDAB_HIGH();
+	LCD_delay_us(5) ;
+	IIC_SDAB_LOW();
+	LCD_delay_us(5) ;
+	IIC_SCLB_LOW();
+}
+
+static __bool i2cBStart( void )
+{
+	IIC_SDAB_HIGH();
+	IIC_SCLB_HIGH();
+	LCD_delay_us(5) ;
+	IIC_SDAB_INPUT_SETUP();
+	if(CHECK_SDAB_HIGH())
+	{
+		IIC_SDAB_OUTPUT_SETUP();
+		{
+			i2cBStartA();
+			return(1);
+		}
+	}
+	return(0);
+}
+
+static void i2cBStop(void)
+{
+   IIC_SDAB_OUTPUT_SETUP();
+   IIC_SDAB_LOW();
+   LCD_delay_us(5) ;
+   IIC_SCLB_HIGH();
+   LCD_delay_us(5) ;
+   IIC_SDAB_HIGH();
+   LCD_delay_us(5) ;
+}
