+/*
+*******************************************************************************
+*                     OSAL_RegISR
+*
+* Description:
+*    
+*
+* Parameters:
+*    irqno    	    input.  
+*    flags    	    input.  0
+*    Handler  	    input.  
+*    pArg 	        input.  
+*    DataSize 	    input.  
+*    prio	        input.  
+
+*
+* Return value:
+*     
+*
+* note:
+*    typedef __s32 (*ISRCallback)( void *pArg)
+*
+*******************************************************************************
+*/
+int OSAL_RegISR(__u32 IrqNo, __u32 Flags,ISRCallback Handler,void *pArg,__u32 DataSize,__u32 Prio)
+{
+    return request_irq(IrqNo, (irq_handler_t)Handler, IRQF_DISABLED, "dev_name", pArg);
+}
+
+/*
+*******************************************************************************
+*                     OSAL_UnRegISR
+*
+* Description:
+*    
+*
+* Parameters:
+*    irqno    	input.  
+*    handler  	input.  
+*    Argment 	input.  
+*
+* Return value:
+*    void
+*
+* note:
+*    void
+*
+*******************************************************************************
+*/
+void OSAL_UnRegISR(__u32 IrqNo, ISRCallback Handler, void *pArg)
+{
+    free_irq(IrqNo, pArg);
+}
+
+/*
+*******************************************************************************
+*                     OSAL_InterruptEnable
+*
+* Description:
+*    
+*
+* Parameters:
+*    irqno input.  
+*
+* Return value:
+*    void
+*
+* note:
+*    void
+*
+*******************************************************************************
+*/
+void OSAL_InterruptEnable(__u32 IrqNo)
+{
+    enable_irq(IrqNo);
+}
+
+/*
+*******************************************************************************
+*                     OSAL_InterruptDisable
+*
+* Description:
+*    
+*
+* Parameters:
+*     irqno input.  
+*
+* Return value:
+*    void
+*
+* note:
+*    void
+*
+*******************************************************************************
+*/
+void OSAL_InterruptDisable(__u32 IrqNo)
+{
+    disable_irq(IrqNo);
+}
