+__u32 LCD_CPU_IO_extend_16b(__u32 value)
+{
+	return
+		  ((value & 0xfc00)<<8)
+		| ((value & 0x0300)<<6)
+		| ((value & 0x00e0)<<5)
+		| ((value & 0x001f)<<3);
+}
+
+__u32 LCD_CPU_IO_shrink_16b(__u32 value)
+{
+	return
+		  ((value & 0xfc0000)>>8)
+		| ((value & 0x00c000)>>6)
+		| ((value & 0x001c00)>>5)
+		| ((value & 0x0000f8)>>3);
+}
