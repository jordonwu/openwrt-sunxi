+    IIC_Write(0xfc, 0xf833, 0xc2);
+    IIC_Write(0xfc, 0xf831, 0xf0);
+    IIC_Write(0xfc, 0xf840, 0x80);
+    IIC_Write(0xfc, 0xf881, 0xec);
+}
