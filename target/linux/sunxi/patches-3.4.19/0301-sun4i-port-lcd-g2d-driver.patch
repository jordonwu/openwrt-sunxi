+__s32 TCON0_select_src(__u32 sel, __u8 src)
+{
+    __u32 tmp;
+
+    tmp = LCDC_RUINT32(sel, LCDC_CTL_OFF);
+    tmp = tmp&0xffbffffc;
+    switch(src)
+    {
+        case LCDC_SRC_DE1:
+             tmp = tmp|0x00;
+             break;
+
+        case LCDC_SRC_DE2:
+             tmp = tmp|0x01;
+             break;
+
+        case LCDC_SRC_DMA:
+             tmp = tmp|0x02;
+             break;
+
+        case LCDC_SRC_WHITE:
+             tmp = tmp|0x00400003;
+             break;
+
+        case LCDC_SRC_BLACK:
+             tmp = tmp|0x03;
+             break;
+    }
+	LCDC_WUINT32(sel,LCDC_CTL_OFF,tmp);
+    return 0;
+}
