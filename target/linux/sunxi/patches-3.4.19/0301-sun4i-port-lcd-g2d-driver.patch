index 0000000..ec0bf5f
--- /dev/null
+++ b/drivers/video/sun4i/disp/de_bsp/de/disp_scaler.c
@@ -0,0 +1,1227 @@
+
+#include "disp_scaler.h"
+#include "disp_display.h"
+#include "disp_event.h"
+#include "disp_layer.h"
+#include "disp_clk.h"
+#include "disp_lcd.h"
