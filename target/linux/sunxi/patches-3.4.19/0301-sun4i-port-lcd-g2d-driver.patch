+		break;
+
+		case	0x4:
+			scal_dev[sel]->vpp_dcti.bits.dcti_gain = 32;
+			scal_dev[sel]->vpp_dcti.bits.dcti_path_limit = 5;
+			scal_dev[sel]->vpp_dcti.bits.uv_same_sign_maxmin_mode_sel = 1;
