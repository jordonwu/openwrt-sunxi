+static __bool i2cBLocateSubAddr(__u8 slave_addr, __u8 sub_addr)
+{
+	register __u8 i;
+	__u32 j;
+
+	for (i=0; i<3; i++)
+	{
+		//Start I2C
+
+		if (i2cBStart())
+		{         //printk("-------------Start I2C OK-----------\n");
+
+			if (i2cBTransmit(slave_addr))
+			{  //printk("-------------SLAVE ADDR SEND OK-----------\n");
+
+
+
+					if (i2cBTransmitSubAddr(sub_addr))
+						 {
+						  //printk("-------------ADDR SEND OK-----------\n");
+						  return(1);
+				     }
+			}
+		}
+		i2cBStop();
+	}
+
+	return(0);
+}
