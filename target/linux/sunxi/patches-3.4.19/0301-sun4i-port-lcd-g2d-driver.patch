+int Fb_blank(int blank_mode, struct fb_info *info)
+{
+    __u32 sel = 0;
+
+	__inf("Fb_blank,mode:%d\n",blank_mode);
+
+    for(sel = 0; sel < 2; sel++)
+    {
+        if(((sel==0) && (g_fbi.fb_mode[info->node] != FB_MODE_SCREEN1))
+            || ((sel==1) && (g_fbi.fb_mode[info->node] != FB_MODE_SCREEN0)))
