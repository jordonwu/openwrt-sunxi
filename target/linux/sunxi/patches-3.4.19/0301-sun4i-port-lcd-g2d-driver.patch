+__s32 DE_BE_Layer_Set_Framebuffer(__u32 sel, __u8 layidx, layer_src_t *layer_fb)
+{
+	__s32 bpp;
+	__u32 addr;
+
+	bpp = DE_BE_Format_To_Bpp(sel, layer_fb->format);
