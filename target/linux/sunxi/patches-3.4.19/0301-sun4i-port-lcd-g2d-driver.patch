+        		}
+
+        		val = (htransp<<24) | (hred<<16) | (hgreen<<8) |hblue;
+        		BSP_disp_set_palette_table(sel, &val, (cmap->start + j) * 4, 4);
+        	}
+    	}
+	}
+	return 0;
+}
